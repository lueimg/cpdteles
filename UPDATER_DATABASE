CREATE TABLE 

CREATE TABLE `cpdteles`.`equisag` (
	`cequisag` int(11) NOT NULL AUTO_INCREMENT,
	`gruequi` int(11) DEFAULT NULL,
	`ccurric` varchar(9) DEFAULT NULL,
	`ccurso` varchar(9) DEFAULT NULL,
	`cmodulo` varchar(9) DEFAULT NULL,
	`ccurria` varchar(9) DEFAULT NULL,
	`ccursoa` varchar(9) DEFAULT NULL,
	`cmoduloa` varchar(9) DEFAULT NULL,
	`estide` char(1) DEFAULT NULL COMMENT 'estide : estado de indentificacion r : regular , i: irregular',
	`cestado` char(1) DEFAULT NULL,
	`cusuari` varchar(20) DEFAULT NULL,
	`fusuari` datetime DEFAULT NULL,
	`cusuariu` varchar(20) DEFAULT NULL,
	`fusuariu` datetime DEFAULT NULL,
	PRIMARY KEY (`cequisag`)
) ENGINE=`InnoDB` AUTO_INCREMENT=1 DEFAULT CHARACTER SET latin1 COLLATE latin1_swedish_ci ROW_FORMAT=COMPACT COMMENT='' CHECKSUM=0 DELAY_KEY_WRITE=0;